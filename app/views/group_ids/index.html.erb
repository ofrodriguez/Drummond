<p id="notice"><%= notice %></p>

<h1>Listing Group Ids</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @group_ids.each do |group_id| %>
      <tr>
        <td><%= group_id.name %></td>
        <td><%= link_to 'Show', group_id %></td>
        <td><%= link_to 'Edit', edit_group_id_path(group_id) %></td>
        <td><%= link_to 'Destroy', group_id, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Group', new_group_id_path %>
